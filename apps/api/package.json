    "@ts-rest/nest": "^3.52.1",
    "@types/pg": "^8.11.11",
    "@workspace/contracts": "workspace:*",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "ebisu-js": "^2.1.3",
    "extract-first-json": "^2.0.2",
    "kysely": "^0.27.6",
